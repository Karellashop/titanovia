   </div>\n  </div>\n</div>\n\n<script>\n(function(){\n  const wrap = document.getElementById('zb-crsl');\n  const track = document.getElementById('zb-track');\n  const dots  = document.getElementById('zb-dots');\n  if(!wrap || !track) return;\n\n  const first = () => track.querySelector('.card');\n  const gap   = () => parseFloat(getComputedStyle(track).gap || 0);\n  const cardW = () => { const c = first(); return c ? c.getBoundingClientRect().width + gap() : 360; };\n  const pad   = () => parseFloat(getComputedStyle(track).paddingLeft || 0);\n\n  function setPad(){\n    const c = first(); if(!c) return;\n    const view = wrap.getBoundingClientRect().width;\n    const p = Math.max(10,(view - c.getBoundingClientRect().width)/2);\n    track.style.setProperty('--sidepad', p + 'px');\n  }\n  function pages(){\n    const tot = track.querySelectorAll('.card').length;\n    const view = wrap.getBoundingClientRect().width;\n    const per = Math.max(1, Math.floor(view / cardW()));\n    return Math.max(1, Math.ceil(tot/per));\n  }\n  function buildDots(){\n    dots.innerHTML = '';\n    for(let i=0;i<pages();i++){ const d=document.createElement('div'); d.className='dot'; dots.appendChild(d); }\n    updateDot();\n  }\n\n  function cur(){ return Math.round((track.scrollLeft - pad()) / cardW()); }\n  function updateDot(){\n    const pg = Math.min(pages()-1, Math.max(0, cur()));\n    [...dots.children].forEach((d,i)=>d.classList.toggle('on', i===pg));\n  }\n  function go(dir){\n    const target = (track.scrollLeft - pad()) + dir*cardW();\n    const min = pad();\n    const max = track.scrollWidth - track.clientWidth - pad();\n    const clamped = Math.max(min, Math.min(max, pad() + target));\n    track.scrollTo({ left: clamped, behavior:'smooth' });\n  }\n\n  const prev = wrap.querySelector('.prev'), next = wrap.querySelector('.next');\n  prev && prev.addEventListener('click', ()=>go(-1));\n  next && next.addEventListener('click', ()=>go(1));\n\n  // fl√®ches mobiles overlay\n  const mPrev = wrap.querySelector('.m-prev');\n  const mNext = wrap.querySelector('.m-next');\n  mPrev && mPrev.addEventListener('click', ()=>go(-1));\n  mNext && mNext.addEventListener('click', ()=>go(1));\n\n  // iOS: priorit√© au carrousel\n  let startX=0, startY=0;\n  track.addEventListener('touchstart', e=>{ const t=e.touches[0]; startX=t.clientX; startY=t.clientY; }, {passive:true});\n  track.addEventListener('touchmove', e=>{\n    const t=e.touches[0];\n    const dx=Math.abs(t.clientX-startX), dy=Math.abs(t.clientY-startY);\n    if(dx>dy){ e.stopPropagation(); }\n  }, {passive:true});\n\n  dots.addEventListener('click', e=>{\n    if(!e.target.classList.contains('dot')) return;\n    const idx = [...dots.children].indexOf(e.target);\n    const pos = pad() + idx*cardW();\n    track.scrollTo({ left: pos, behavior:'smooth' });\n  });\n\n  function init(){\n    setPad();\n    buildDots();\n    track.scrollLeft = pad(); // d√©marre sur la 1 ≥·µâ carte\n    updateDot();\n  }\n  init();\n  window.addEventListener('resize', init, {passive:true});\n  track.addEventListener('scroll', ()=> requestAnimationFrame(updateDot), {passive:true});\n})();\n</script>",
