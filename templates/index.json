            "liquid": "<style>\n  .lx-slider{overflow:hidden;position:relative;width:100%}\n  .lx-track{display:flex;transition:transform .5s ease}\n  .lx-slide{min-width:100%;box-sizing:border-box;padding:0 6px}\n\n  .lx-testimonial{display:flex;gap:14px;align-items:flex-start;background:#fff;border:1px solid #eee;border-radius:14px;padding:16px 18px;box-shadow:0 4px 14px rgba(0,0,0,.05)}\n  .lx-testimonial__avatar{width:48px;height:48px;border-radius:9999px;flex:0 0 48px;overflow:hidden;border:2px solid #f1f1f1}\n  .lx-testimonial__avatar img{width:100%;height:100%;object-fit:cover}\n  .lx-testimonial__body{line-height:1.45;flex:1}\n  .lx-testimonial__name{font-weight:700;font-size:14px;margin:0 0 2px;display:flex;gap:6px;align-items:center;flex-wrap:wrap}\n  .lx-verified{opacity:.85;font-weight:600}\n  .lx-time{opacity:.65;font-size:12px;margin-left:auto}\n  .lx-testimonial__stars{display:flex;gap:2px;margin:4px 0 8px}\n  .lx-testimonial__text{margin:0;color:#333;font-size:14px}\n\n  @media (min-width:740px){\n    .lx-testimonial{padding:18px 22px}\n    .lx-testimonial__text{font-size:15px}\n  }\n\n  /* Dots */\n  .lx-nav{display:flex;justify-content:center;gap:8px;margin-top:14px}\n  .lx-dot{width:8px;height:8px;border-radius:50%;background:#ccc;cursor:pointer;transition:.3s}\n  .lx-dot.active{background:#FFD166}\n</style>\n\n<div class=\"lx-slider\" id=\"lxSlider\" aria-roledescription=\"carousel\">\n  {% assign star = '<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"#FFD166\"><path d=\"M12 2.5l2.9 6.1 6.7.6-5 4.4 1.5 6.6L12 17.7 5.9 20.2 7.4 13.6 2.4 9.2l6.7-.6L12 2.5z\"/></svg>' %}\n\n  <div class=\"lx-track\">\n    <!-- Avis 1 -->\n    <div class=\"lx-slide\" role=\"group\" aria-label=\"1 sur 3\">\n      <div class=\"lx-testimonial\">\n        <div class=\"lx-testimonial__avatar\">\n          <img src=\"https://cdn.shopify.com/s/files/1/0969/2843/2465/files/testimonial2_10da188c-163f-4664-ad68-1ff70ee4aece.jpg?v=1758938622\" alt=\"Sandrine S.\" width=\"96\" height=\"96\" loading=\"lazy\">\n        </div>\n        <div class=\"lx-testimonial__body\">\n          <p class=\"lx-testimonial__name\">\n            Sandrine S. <span class=\"lx-verified\">• Vérifié</span>\n            <span class=\"lx-time\">Il y a 9h</span>\n          </p>\n          <div class=\"lx-testimonial__stars\" aria-label=\"5 étoiles\">\n            {{ star }}{{ star }}{{ star }}{{ star }}{{ star }}\n          </div>\n          <p class=\"lx-testimonial__text\">J’étais sceptique au début, mais la qualité m’a bluffée et le support a été top. Je recommande.</p>\n        </div>\n      </div>\n    </div>\n\n    <!-- Avis 2 -->\n    <div class=\"lx-slide\" role=\"group\" aria-label=\"2 sur 3\">\n      <div class=\"lx-testimonial\">\n        <div class=\"lx-testimonial__avatar\">\n          <img src=\"https://cdn.shopify.com/s/files/1/0969/2843/2465/files/testimonial7.jpg?v=1758938621\" alt=\"Sophie P.\" width=\"96\" height=\"96\" loading=\"lazy\">\n        </div>\n        <div class=\"lx-testimonial__body\">\n          <p class=\"lx-testimonial__name\">\n            Sophie P. <span class=\"lx-verified\">• Vérifié</span>\n            <span class=\"lx-time\">Il y a 6h</span>\n          </p>\n          <div class=\"lx-testimonial__stars\" aria-label=\"5 étoiles\">\n            {{ star }}{{ star }}{{ star }}{{ star }}{{ star }}\n          </div>\n          <p class=\"lx-testimonial__text\">Je ne savais pas quelle taille choisir. La fiche m’a aidée et le rendu est parfait. Ravie !</p>\n        </div>\n      </div>\n    </div>\n\n    <!-- Avis 3 -->\n    <div class=\"lx-slide\" role=\"group\" aria-label=\"3 sur 3\">\n      <div class=\"lx-testimonial\">\n        <div class=\"lx-testimonial__avatar\">\n          <img src=\"https://cdn.shopify.com/s/files/1/0969/2843/2465/files/testimonial6.jpg?v=1758938623\" alt=\"Thom. M\" width=\"96\" height=\"96\" loading=\"lazy\">\n        </div>\n        <div class=\"lx-testimonial__body\">\n          <p class=\"lx-testimonial__name\">\n            Thom. M <span class=\"lx-verified\">• Vérifié</span>\n            <span class=\"lx-time\">Il y a 10h</span>\n          </p>\n          <div class=\"lx-testimonial__stars\" aria-label=\"5 étoiles\">\n            {{ star }}{{ star }}{{ star }}{{ star }}{{ star }}\n          </div>\n          <p class=\"lx-testimonial__text\">Livraison rapide, support réactif, produit impeccable. Confiance gagnée.</p>\n        </div>\n      </div>\n    </div>\n  </div>\n\n  <!-- Dots -->\n  <div class=\"lx-nav\" aria-hidden=\"true\">\n    <div class=\"lx-dot active\" data-slide=\"0\"></div>\n    <div class=\"lx-dot\" data-slide=\"1\"></div>\n    <div class=\"lx-dot\" data-slide=\"2\"></div>\n  </div>\n</div>\n\n<script>\n(function(){\n  const slider = document.getElementById('lxSlider');\n  const track  = slider.querySelector('.lx-track');\n  const slides = slider.querySelectorAll('.lx-slide');\n  const dots   = slider.querySelectorAll('.lx-dot');\n\n  let current = 0;\n  const total = slides.length;\n  let timer   = null;\n  const INTERVAL = 5000; // vitesse d’auto défilement\n\n  function goTo(i){\n    current = (i + total) % total;\n    track.style.transform = `translateX(-${current * 100}%)`;\n    dots.forEach(d => d.classList.remove('active'));\n    if(dots[current]) dots[current].classList.add('active');\n  }\n\n  function start(){ stop(); timer = setInterval(()=> goTo(current+1), INTERVAL); }\n  function stop(){ if(timer){ clearInterval(timer); timer=null; } }\n\n  dots.forEach((d,idx)=> d.addEventListener('click',()=>{ goTo(idx); start(); }));\n\n  // Pause au survol (desktop)\n  slider.addEventListener('mouseenter', stop);\n  slider.addEventListener('mouseleave', start);\n\n  // Swipe tactile + drag desktop\n  let startX=null, dragging=false;\n  function onStart(e){ dragging=true; startX=(e.touches?e.touches[0].clientX:e.clientX); stop(); }\n  function onEnd(e){\n    if(!dragging) return;\n    const x=(e.changedTouches?e.changedTouches[0].clientX:e.clientX);\n    const dx=x-startX;\n    if(Math.abs(dx)>40){ goTo(current + (dx<0?1:-1)); }\n    start(); dragging=false; startX=null;\n  }\n  slider.addEventListener('touchstart', onStart,{passive:true});\n  slider.addEventListener('touchend', onEnd);\n  slider.addEventListener('mousedown', onStart);\n  window.addEventListener('mouseup', onEnd);\n\n  // Pause si hors écran (perf)\n  if('IntersectionObserver' in window){\n    const io = new IntersectionObserver((entries)=>{\n      entries.forEach(e=> e.isIntersecting ? start() : stop());\n    },{threshold:0.2});\n    io.observe(slider);\n  } else { start(); }\n\n  // Démarrage\n  goTo(0); start();\n})();\n</script>"
