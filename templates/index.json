ius:8px;\n}\n\n/* Cartes ‚Äì vid√©os */\n.lxv-card{\n  flex:0 0 70%;\n  scroll-snap-align:center;\n  background:#fff;\n  border:1px solid var(--ring);\n  border-radius:16px;\n  box-shadow:0 4px 14px rgba(0,0,0,.06);\n  overflow:hidden;\n}\n.lxv-vid-wrap{\n  position:relative;\n  width:100%;\n  aspect-ratio:1/1.35;\n  max-height:300px;\n  background:#faf7ff;\n}\n.lxv-video{\n  width:100%;\n  height:100%;\n  object-fit:cover;\n  display:block;\n  background:#eee;\n}\n\n/* Bouton son bas gauche */\n.lxv-sound-btn{\n  position:absolute;\n  left:10px;\n  bottom:10px;\n  padding:4px 8px;\n  font-size:14px;\n  line-height:1;\n  border:none;\n  border-radius:999px;\n  background:rgba(0,0,0,0.65);\n  color:#fff;\n  cursor:pointer;\n  display:inline-flex;\n  align-items:center;\n  justify-content:center;\n  transition:background .2s, transform .15s;\n}\n.lxv-sound-btn:hover{\n  background:rgba(0,0,0,0.8);\n  transform:translateY(-1px);\n}\n\n/* Dots */\n.lxv-dots{\n  display:flex;\n  gap:6px;\n  justify-content:center;\n  margin-top:8px;\n}\n.lxv-dots .dot{\n  width:6px;\n  height:6px;\n  border-radius:50%;\n  background:#e8d8ff;\n  transition:.2s;\n}\n.lxv-dots .dot.is-active{\n  background:var(--rose);\n}\n\n/* Desktop */\n@media (min-width:900px){\n  .lxv-card{\n    flex:0 0 calc((100% - 42px)/4);\n  }\n  .lxv-vid-wrap{\n    max-height:420px;\n  }\n  .lxv-dots{\n    display:none;\n  }\n}\n</style>\n\n<script>\n(function(){\n  const container = document.querySelector('.lxv-uugc');\n  if(!container) return;\n\n  const track  = container.querySelector('.lxv-carousel');\n  const cards  = container.querySelectorAll('.lxv-card');\n  const dots   = container.querySelectorAll('.lxv-dots .dot');\n  const videos = container.querySelectorAll('.lxv-video');\n\n  if(!track || !cards.length) return;\n\n  // Lazy load + autoplay en boucle\n  const io = new IntersectionObserver((entries)=>{\n    entries.forEach(entry=>{\n      const card = entry.target;\n      const vid  = card.querySelector('.lxv-video');\n      if(!vid) return;\n\n      if(entry.isIntersecting){\n        if(!vid.src && vid.dataset.src){\n          vid.src = vid.dataset.src;\n        }\n        vid.play().catch(()=>{});\n      }\n    });\n  },{threshold:.2});\n\n  cards.forEach(c=>io.observe(c));\n\n  // Dots sur mobile\n  const updateDots = ()=>{\n    if(window.matchMedia('(min-width:900px)').matches) return;\n    const rect = track.getBoundingClientRect();\n    let active = 0, best = Infinity;\n\n    cards.forEach((c,i)=>{\n      const r = c.getBoundingClientRect();\n      const dist = Math.abs((r.left+r.right)/2 - (rect.left+rect.right)/2);\n      if(dist < best){\n        best = dist;\n        active = i;\n      }\n    });\n\n    dots.forEach((d,i)=> d.classList.toggle('is-active', i===active));\n  };\n\n  track.addEventListener('scroll', updateDots, {passive:true});\n  window.addEventListener('resize', updateDots);\n  updateDots();\n\n  // Coupe le son sur toutes les vid√©os sauf une\n  function muteOthers(except){\n    videos.forEach(v=>{\n      if(v !== except){\n        v.muted = true;\n        const btn = v.parentElement.querySelector('.lxv-sound-btn');\n        if(btn) btn.textContent = 'üîá';\n      }\n    });\n  }\n\n  // Gestion du bouton son\n  cards.forEach(card=>{\n    const vid = card.querySelector('.lxv-video');\n    const btn = card.querySelector('.lxv-sound-btn');\n    if(!vid || !btn) return;\n\n    btn.addEventListener('click', (e)=>{\n      e.stopPropagation();\n\n      // S'assure que la vid√©o est bien charg√©e\n      if(!vid.src && vid.dataset.src){\n        vid.src = vid.dataset.src;\n      }\n      vid.play().catch(()=>{});\n\n      if(vid.muted){\n        muteOthers(vid);\n        vid.muted = false;\n        btn.textContent = 'üîä';\n      } else {\n        vid.muted = true;\n        btn.textContent = 'üîá';\n      }\n    });\n  });\n})();\n</script>\n<!-- ===== /+ DE 12 000 MA√éTRES ===== -->"
